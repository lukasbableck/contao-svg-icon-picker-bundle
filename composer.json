{
    "name": "lukasbableck/contao-svg-icon-picker-bundle",
    "type": "contao-bundle",
	"description": "This bundle adds a svg icon picker widget to Contao.",
	"keywords": [
		"svg",
		"icon",
		"fontawesome",
		"picker"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3",
        "enshrined/svg-sanitize": "^0.22",
        "symfony/asset": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "twig/cache-extra": "^3.10",
        "twig/twig": "^3.10"
    },
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.86",
        "vincentlanglet/twig-cs-fixer": "^3.9"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoSVGIconPickerBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoSVGIconPickerBundle\\ContaoManager\\Plugin"
	}
}
